    "devDependencies": {
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.3",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "prettier": "2.8.7",
        "ts-jest": "^29.0.3",
        "typescript": "5.0.2"
